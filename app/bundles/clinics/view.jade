#magazines.row.collapse
	#magazines-header
		a.uno(href=ﬁ.bundles['clinics']) Pequeñas Especies, Mascotas no Convencionales y Fauna Silvestre
	
	#magazines-issues.row(data-equalizer)
		each mag in clinics.Pequenas
			.small-6.medium-4.large-2.columns(style="height:350px;text-align:center",data-equalizer-watch)
				figure.magazine
					a(href="#")
						img(style="height:205px;margin-bottom:15px;margin-top:15px",src=mag.img)
					figcaption
						h4: a(href="#") #{mag.name}

