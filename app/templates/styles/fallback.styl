& > #fallback
	position   : absolute
	display    : block
	background : #543519
	top        : 0
	left       : 0
	width      : 100%
	height     : 100%

	& > img
		width       : 18em
		position    : relative
		display     : block
		top         : 2.5em
		margin      : 0 auto

	& > p
		width       : 100%
		display     : block
		position    : absolute
		top         : 0
		left        : 0
		font-weight : bold
		font-size   : 20pt
		color       : white
		padding     : 5em
		padding-top : 10em

& > #data
& > header
& > #notice
& > #content
& > footer
	display:none