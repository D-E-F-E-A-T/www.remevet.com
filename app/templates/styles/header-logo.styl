&
	position      : absolute
	margin-top    :   5px
	margin-bottom :   5px
	margin-left   :   9px
	height        :  95px
	width         : 161px
	padding       :   0px

	a
		display         : block
		position        : absolute
		width           : 100%
		height          : 100%
		top             : 0
		left            : 0
		overflow        : hidden
		text-indent     : -300em
		background      : url('/static/images/header/logo-white.svg') no-repeat top left
		background-size : contain

	// Android 2.3 does not support SVG
	/.no-svg & a
		background-image    : url('/static/images/header/logo-white.png')
		background-position : center center
		background-size     : 100%